<br/><%= label_tag(:version, "Version:") %>
<br/><%= text_field_tag(:version, spec.version.presence || "v1.0", class: "form-control") %>
<br/><%= label_tag(:title, "Title:") %>
<br/><%= text_field_tag(:title, spec.title.presence || "RingCentral API", class: "form-control") %>
<br/><%= label_tag(:description, "Description:") %>
<br/><%= text_area_tag(:description, spec.description.presence || "RingCentral Connect Platform API", class: 'article-editor', placeholder: 'Styling with Markdown is supported') %>
<br/><%= label_tag(:termsOfService, "Terms of Service:") %>
<br/><%= text_field_tag(:termsOfService, spec.termsOfService.presence || "http://www.ringcentral.com/legal/eulatos.html", class: "form-control") %>
<br/><%= label_tag(:host, "Host:") %>
<br/><%= text_field_tag(:host, spec.host.presence || "platform.devtest.ringcentral.com", class: "form-control") %>
<br/><%= label_tag(:basePath, "Base Path:") %>
<br/><%= text_field_tag(:basePath, spec.basePath.presence || "/", class: "form-control") %>
<br/><%= label_tag(:schemes, "Schemes:") %>
<br/><%= text_field_tag(:schemes, spec.schemes.presence || "https", class: "form-control") %>
<br/><%= label_tag(:produces, "Produces:") %>
<br/><%= text_field_tag(:produces, spec.produces.presence || "application/json", class: "form-control") %>
<br/><%= label_tag(:consumes, "Consumes:") %>
<br/><%= text_field_tag(:consumes, spec.consumes.presence || "application/json", class: "form-control") %>
